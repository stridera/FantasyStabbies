{
  "name": "fantasy_website",
  "version": "1.0.0",
  "private": true,
  "description": "Reddit /r/fantasy stabby voting site.",
  "scripts": {
    "start-server": "cross-env NODE_ENV=development nodemon server/server.js --watch 'server/*'",
    "start-prod-server": "cross-env NODE_ENV=production node server/server.js",
    "start-front": "react-scripts start",
    "build-front": "react-scripts build",
    "migrate": "knex --knexfile=./server/knexfile.js migrate:latest",
    "rollback": "knex --knexfile=./server/knexfile.js migrate:rollback",
    "eject": "react-scripts eject",
    "test": "react-scripts test",
    "start": "concurrently \"yarn run start-server\" \"yarn run start-front\" --kill-others",
    "start-prod": "concurrently \"yarn run start-prod-server\" \"yarn run start-front\" --kill-others"
  },
  "proxy": "http://localhost:5000",
  "author": "Matthew Strider Jones (stridera)",
  "license": "MIT",
  "dependencies": {
    "@hookform/resolvers": "^0.1.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@reduxjs/toolkit": "^1.4.0",
    "axios": "^0.19.2",
    "camaro": "^6.0.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "http-proxy-middleware": "^1.0.5",
    "knex": "^0.21.14",
    "lodash": "^4.17.20",
    "moment": "^2.24.0",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "objection": "^2.2.3",
    "passport": "^0.4.1",
    "passport-reddit": "^0.2.4",
    "pg": "^8.5.1",
    "react": "^16.13.1",
    "react-date-picker": "^8.0.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.4.0",
    "react-moment": "^0.9.7",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "slugify": "^1.4.5",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^26.0.9",
    "axios-mock-adapter": "^1.18.2",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.4",
    "react-scripts": "3.4.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devServer": {
    "compress": true,
    "public": "rfantasy.com"
  }
}
